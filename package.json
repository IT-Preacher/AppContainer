{
  "name": "appcon",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "11": "react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "11-pro": "react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "se": "react-native run-ios --simulator=\"iPhone SE\"",
    "ia2": "react-native run-ios --simulator=\"iPad Air 2\"",
    "ip4": "react-native run-ios --simulator=\"iPad Pro 4th\"",
    "ios": "react-native run-ios --simulator 'iPhone 13'",
    "ios-clear": "rm -rf ios/build && react-native run-ios --simulator 'iPhone 13'",
    "android": "react-native run-android",
    "start": "react-native start --reset-cache",
    "android-release-aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && open ./app/build/outputs/bundle/release/",
    "android-menu": "adb shell input keyevent 82",
    "xcode": "open ios/Daily.xcworkspace",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@eva-design/eva": "^2.2.0",
    "@react-navigation/native": "^6.1.9",
    "@ui-kitten/components": "5.3.1",
    "@ui-kitten/eva-icons": "5.3.1",
    "@ui-kitten/metro-config": "5.3.1",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
